  "packageManager": "pnpm@9.15.9",
  "scripts": {
    "build": "PRISMA_CLIENT_FORCE_WASM=true pnpm prisma generate && tsc -p tsconfig.build.json",
    "start": "pnpm ts-node --swc src/index.ts",
    "dev": "pnpm start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint:check": "eslint --report-unused-disable-directives --cache --ext js,ts,jsx,tsx . --max-warnings 0",
    "eslint:fix": "pnpm run eslint:check --fix",
    "lint": "pnpm prettier:check && pnpm eslint:check",
    "lint:fix": "pnpm prettier:fix && pnpm eslint:fix",
    "lb": "pnpm lint:fix && pnpm build",
    "prettier:check": "prettier --cache --cache-strategy metadata --check \"./**/*.{js,ts,jsx,tsx,md,json,html}\"",
    "prettier:fix": "prettier --cache --cache-strategy metadata --write \"./**/*.{js,ts,jsx,tsx,md,json,html}\" --log-level=warn"
  },
